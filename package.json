    "mockjs": "^1.1.0",
    "pinia": "^2.1.7",
    "vue": "^3.3.4",
    "vue-echarts": "^6.6.1",
    "vue-i18n": "^9.6.5",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@arco-design/web-vue": "^2.53.1",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/lodash": "^4.14.201",
